{
  "$schema": "https://staticwebapp.net/schema/latest/staticwebapp.config.schema.json",
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/assets/*", "/images/*", "/fonts/*"]
  },
  "responseOverrides": {
    "404": { "rewrite": "/index.html", "statusCode": 200 }
  }
}
